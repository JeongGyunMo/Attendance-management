<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jp.ac.service.dao.serviceDAO">

 <select id="selectProjectList" resultType="entity">
	SELECT 	PRO_ID, 
		PRO_TITLE, 
		PRO_BODY, 
		PRO_DATE 
		FROM TB_PROJECT
	ORDER BY PRO_ID DESC
 </select>

</mapper>